{
    "name": "ironedge/file-utils",
    "description": "File utilities.",
    "keywords": ["xml", "json", "file"],
    "homepage": "http://www.ironedge.com",
    "type": "library",
    "license": "MIT",
    "version":          "1.0.0",
    "authors": [
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com",
            "homepage": "http://www.ironedge.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/ironedgesoftware/file-utils/issues"
    },
    "require": {
        "php": "^5.3.2 || ^7.0",
        "justinrainbow/json-schema": "^1.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0"
    },
    "autoload": {
        "psr-4": { "IronEdge\\Component\\FileUtils\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "IronEdge\\Component\\FileUtils\\Test\\": "tests/Test" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}