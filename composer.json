{
    "name": "ironedge/file-utils",
    "description": "File utilities.",
    "keywords": ["xml", "json", "file"],
    "homepage": "http://www.ironedgesoftware.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com",
            "homepage": "http://www.ironedgesoftware.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/ironedgesoftware/file-utils/issues"
    },
    "require": {
        "php": "^5.6.0 || ^7.0",
        "symfony/options-resolver": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0"
    },
    "suggest": {
        "seld/jsonlint": "Enabling JSON lint validation.",
        "justinrainbow/json-schema": "Enabling JSON schema validation.",
        "symfony/yaml": "Enabling YAML files handling."
    },
    "autoload": {
        "psr-4": { "IronEdge\\Component\\FileUtils\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "IronEdge\\Component\\FileUtils\\Test\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}